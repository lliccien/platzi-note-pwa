{
    "index": "/index.html",
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/favicon.ico",
                "/index.html",
                "/*.css",
                "/*.js"
            ]
        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/assets/**"
            ]
        }
    }],
    "dataFroups": [{
        "name": "FirebaseAPI",
        "urls": [
            "https://pwa-platzi.firebaseio.com/**"
        ],
        "cacheConfig": {
            "strategy": "freshness",
            "maxAge": "2h",
            "maxSize": 100,
            "timeout": "5s"
        }
    }]
}